            &lt;!&ndash; 该参数默认为false &ndash;&gt;
            &lt;!&ndash; 设置为true时，使用RowBounds分页会进行count查询 &ndash;&gt;
            <property name="rowBoundsWithCount" value="true"/>
        </plugin>
    </plugins>-->
</configuration>